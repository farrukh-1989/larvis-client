{
  "files": [],
  "references": [{ "path": "./tsconfig.app.json" }, { "path": "./tsconfig.node.json" }],
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["node", "jest", "@testing-library/jest-dom"],
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": ["./src/*", "./dist/*", ""],
      "@Assets/*": ["src/assets/*"],
      "@Components/*": ["src/components/*"],
      "@Features/*": ["src/features/*"],
      "@Routes/*": ["src/routes/*"],
      "@Store/*": ["src/store/*"],
      "@Utils/*": ["src/utils/*"],
      "@Types/*": ["src/types/*"]
    }
  }
}
